 CREATE PROCEDURE dbo.LeerPart
as
begin
 select* from	PARTICIPANTE
 End


 Create procedure dbo.agregarPart	
 (
 @PART_RUT Varchar(10),
 @PART_NOMBRE VARCHAR(10),
 @PART_FONO VARCHAR(10),
 @PART_EMAIL VARCHAR(10),
 @PART_INSTITUCION VARCHAR(10)
 )
 AS
 BEGIN
 INSERT INTO PARTICIPANTE VALUES
 (@PART_RUT,@PART_NOMBRE,@PART_FONO,@PART_EMAIL,@PART_INSTITUCION)
 END
